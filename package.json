  "devDependencies": {
    "body-parser": "^1.15.2",
    "errorhandler": "^1.4.3",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "jade": "^1.11.0",
    "log4js": "^0.6.38",
    "serve-favicon": "^2.3.0"
